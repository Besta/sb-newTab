{
  "author": "Simone Bestazza",
  "name": "sb-newTab",
	"manifest_version": 2,
	"version": "0.0.1",
  "description": "A very simple new tab",

  "chrome_url_overrides" : {
    "newtab": "sb-newTab.html"
  },

	"web_accessible_resources": [
		"script/*",
		"style/*",
    "font/*",
    "img/*"
  ],

  "permissions": [
    "topSites",
		"https://api.coinbase.com/"
  ]
}
